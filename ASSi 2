#include <iostream>
using namespace std;

class studData {
    static int counter;
private:
    int rno;
    char name[20];
    char DOB[10];
    char div;
    char address[20];
    int phno;
    char standard;

public:
       static void count(){
           counter++;
           cout<<"object"<<counter<<"created"<<endl;
       }
        void inputData() {
        cout << "Enter your RollNo :" << endl;
        cin >> rno;
        cout << "Enter your name :" << endl;
        cin >> name;
        cout << "Enter your Date of Birth :" << endl;
        cin >> DOB;
        cout << "div :" << endl;
        cin >> div;
        cout << "Enter your address :" << endl;
        cin >> address;
        cout << "Enter your Phone NO :" << endl;
        cin >> phno;
        cout << "Enter your Standard you are studying :" << endl;
        cin >> standard;
    }
    void displayData(){
    cout << "Your roll no is: " << rno << endl;
    cout << "Your name is: " << name << endl;
    cout << "Your Date of Birth is: " << DOB << endl;
    cout << "Your division is: " << div << endl;
    cout << "Your address is: " << address << endl;
    cout << "Your Phone NO is: " << phno << endl;
    cout << "You are studying in Standard: " << standard << endl;
    }
    
};

int studData::counter = 0;  // Definition of the static counter variable

int main() {
    
    studData s;
    s.inputData();
    s.displayData();

    return 0;
}
